{
  "name": "dnh",
  "version": "1.0.0",
  "description": "Touhou Danmakufu syntax highlighting for Visual Studio Code",
  "keywords": [
    "front-end",
    "boilerplate",
    "template",
    "generator",
    "webpack"
  ],
  "author": "Michael Serajnik <ms@mserajnik.at>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mserajnik/dnh.git"
  },
  "bugs": {
    "url": "https://github.com/mserajnik/dnh/issues"
  },
  "publisher": "mserajnik",
  "displayName": "dnh",
  "engines": {
    "vscode": "^1.24.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#263238",
    "theme": "light"
  },
  "contributes": {
    "languages": [{
      "id": "dnh",
      "aliases": ["Touhou Danmakufu", "dnh"],
      "extensions": [".dnh"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "dnh",
      "scopeName": "source.dnh",
      "path": "./syntaxes/dnh.tmLanguage.json"
    }]
  }
}
